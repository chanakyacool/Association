
<% @articles.each do |article| %>
  <div class="well">
    <strong><%=link_to  article.title, article %></strong><br><br>
   
    <span style=	"color:#09346e"><%= article.content %></span> <br><hr>
 
    <%= link_to 'Edit', edit_article_path(article) %> |
    <%= link_to 'Destroy', article, method: :delete, data: { confirm: 'Are you sure?' }  %> <br>
  </div>
<% end %>


